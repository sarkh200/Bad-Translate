{
  "manifest_version": 3,
  "name": "Bad Translate",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": [
    "tabs",
    "scripting"
  ],
  "host_permissions": [
    "https://translate.google.com/*"
  ],
  "icons": {
    "16": "./icons/icon16.png",
    "32": "./icons/icon32.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html"
  }
}